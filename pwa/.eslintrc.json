{
  "root": true,
  "plugins": [],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended",
    "plugin:react-hooks/recommended",
    "react-app",
    "next"
  ],
  "rules": {
    "@next/next/no-img-element": "off",
    "@typescript-eslint/no-explicit-any": "off",
    "prettier/prettier": [
      "warn"
    ]
  },
  "settings": {
   "next": {
      "rootDir": "app"
    }
  }
}
